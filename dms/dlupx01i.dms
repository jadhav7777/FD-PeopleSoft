-- Delete and import the Core Application Messages Tables

SET LOG DLUPX01I.log;
SET INPUT DLUPX01.dat;
SET NO RECORD;

DELETE FROM PSMSGSETDEFN WHERE
  MESSAGE_SET_NBR>=1000 AND
  MESSAGE_SET_NBR < 20000;

DELETE FROM PSMSGSETLANG WHERE
  MESSAGE_SET_NBR>=1000 AND
  MESSAGE_SET_NBR < 20000;

DELETE FROM PSMSGCATDEFN WHERE
  MESSAGE_SET_NBR>=1000 AND
  MESSAGE_SET_NBR < 20000;

DELETE FROM PSMSGCATLANG WHERE
  MESSAGE_SET_NBR>=1000 AND
  MESSAGE_SET_NBR < 20000;

IMPORT *;

COMMIT;

